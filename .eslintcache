oading, repos} = githubContext\n\n  useEffect(() => {\n    githubContext.getUser(match.params.login)\n    githubContext.getUserRepos(match.params.login)\n  }, [])\n\n  const {\n    name,\n    avatar_url,\n    location,\n    bio,\n    blog,\n    login,\n    html_url,\n    followers,\n    following,\n    company,\n    public_repos,\n    public_gists,\n    hireable,\n  } = user\n\n  if (loading) return <Spinner />\n\n  return (\n    <>\n      <Link to='/' className='btn btn-light mr-4'>\n        Back to Search\n      </Link>\n      Hireable:{' '}\n      {hireable ? (\n        <i className='fas fa-check text-success' />\n      ) : (\n        <i className='fas fa-times-circle text-danger' />\n      )}\n      <div className='card mt-4'>\n        <div className='p-4'>\n          <div className='row'>\n            <div className='col text-center'>\n              <img\n                src={avatar_url}\n                alt='img'\n                className='rounded-circle'\n                style={{width: '100px'}}\n              />\n              <h1>{name}</h1>\n              <p>{location}</p>\n            </div>\n            <div className='col'>\n              {bio && (\n                <>\n                  <h3>Bio:</h3> <p>{bio}</p>\n                </>\n              )}\n\n              <a href={html_url} className='btn btn-dark p-2 m-4'>\n                Visit GitHub Profile\n              </a>\n\n              <ul>\n                {login && (\n                  <li>\n                    <strong> Username: </strong>\n                    {login}\n                  </li>\n                )}\n\n                {blog && (\n                  <li>\n                    <strong> Website: </strong>\n                    <a href={blog}>{blog}</a>\n                  </li>\n                )}\n\n                {company && (\n                  <li>\n                    <strong> Company: </strong>\n                    {company}\n                  </li>\n                )}\n              </ul>\n            </div>\n          </div>\n        </div>\n      </div>\n      <div className='card text-center mt-4 p-4 d-inline-block w-100 mt-4'>\n        <div className='badge badge-primary w-10 m-2 p-2'>\n          Followers: {followers}\n        </div>\n        <div className='badge badge-success w-10 m-2 p-2'>\n          Following: {following}\n        </div>\n        <div className='badge badge-danger w-10 m-2 p-2'>\n          Public Repos: {public_repos}\n        </div>\n        <div className='badge badge-dark w-10 m-2 p-2'>\n          Public Gists: {public_gists}\n        </div>\n      </div>\n      <Repos repos={repos} loading={loading} />\n    </>\n  )\n}\n\nexport default User\n","/Users/ethancannelongo/Desktop/Liberty Classes/react-projects/github-finder/src/components/repos/Repos.js",["105"],"import React, {useContext} from 'react'\nimport PropTypes from 'prop-types'\nimport RepoItem from './RepoItem'\nimport GithubContext from '../../context/github/githubContext'\n\nfunction Repos() {\n  const githubContext = useContext(GithubContext)\n\n  return (\n    <>\n      <h3 className='mt-4'>Repos</h3>\n      <div className='card'>\n        {githubContext.userRepos.map(repo => (\n          <RepoItem key={repo.id} repo={repo} />\n        ))}\n      </div>\n    </>\n  )\n}\n\nexport default Repos\n","/Users/ethancannelongo/Desktop/Liberty Classes/react-projects/github-finder/src/components/repos/RepoItem.js",[],"/Users/ethancannelongo/Desktop/Liberty Classes/react-projects/github-finder/src/context/github/githubState.js",["106","107","108","109","110","111","112"],"/Users/ethancannelongo/Desktop/Liberty Classes/react-projects/github-finder/src/context/github/githubContext.js",[],"/Users/ethancannelongo/Desktop/Liberty Classes/react-projects/github-finder/src/context/github/githubReducer.js",["113"],"//GithubReducer actually changes the state based on the data received from the payload provided in the dispatch functions in GithubState\n\nimport {\n  SEA